{
    "name": "markdown-videos",
    "private": true,
    "version": "0.0.0",
    "packageManager": "pnpm@9.0.6",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "build": "turbo run build --env-mode=loose",
        "api:install": "turbo run api:install --env-mode=loose",
        "dev": "turbo dev --parallel --env-mode=loose",
        "start": "turbo start --parallel --env-mode=loose",
        "preview": "turbo preview --parallel --env-mode=loose",
        "test": "turbo run test --env-mode=loose",
        "test:verbose": "turbo run test:verbose --env-mode=loose",
        "lint": "turbo run lint --env-mode=loose",
        "clean:workspaces": "turbo clean --env-mode=loose"
    },
    "devDependencies": {
        "dotenv-cli": "^7.4.2",
        "turbo": "^2.1.2"
    }
}
